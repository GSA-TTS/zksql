location=local # or distributed
schema-name=healthlnk
# schemas supported: healthlnk, tpch; also may be overriden with Java system properties

distributed-eval-enabled=false

# config filename format: conf/setup.$location"-"$schemaName
#e.g., local-healthlnk-configuration=conf/setup.local


#log information - info, fine, finer, finest
log-level=fine

#query planner information
sliced-execution=false
slice-parallelism=2
semijoin-execution=false

maven-location=/usr/local/bin/mvn